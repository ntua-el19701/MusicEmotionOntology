              <owl:withRestrictions>
                <rdf:Description>
                  <rdf:first>
                    <rdf:Description>
                      <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">-3.9</xsd:maxExclusive>

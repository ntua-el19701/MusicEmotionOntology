<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://www.semanticweb.org/musicEmotionOntology"
         xmlns="http://www.semanticweb.org/musicEmotionOntology#">

<owl:Ontology rdf:about="http://www.semanticweb.org/musicEmotionOntology"/>

<owl:ObjectProperty rdf:about="#hasMusicalFeature">
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="#MusicalFeature"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class connecting a track to their musical features</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#triggers">
  <rdfs:domain rdf:resource="#MusicalFeature"/>
  <rdfs:range rdf:resource="#EmotionalEffect"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class that defines the relationship between a musical feature and the emotions it triggers</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#meterHasTrack">
  <rdfs:domain rdf:resource="#Meter"/>
  <rdfs:range rdf:resource="#Track"/>
  <owl:inverseOf rdf:resource="#trackHasMeter"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class that defines the relationship between meter and Track</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#trackHasMeter">
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="#Meter"/>
  <owl:inverseOf rdf:resource="#meterHasTrack"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasNext">
  <rdfs:domain rdf:resource="#Meter"/>
  <rdfs:range rdf:resource="#Meter"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class that defines which meter is the next one of this meter</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isFollowedByMeter">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
  <rdfs:domain rdf:resource="#Meter"/>
  <rdfs:range rdf:resource="#Meter"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasNextMeter">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:subPropertyOf rdf:resource="#isFollowedByMeter"/>
  <rdfs:domain rdf:resource="#Meter"/>
  <rdfs:range rdf:resource="#Meter"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#followsMeter">
  <rdfs:domain rdf:resource="#Meter"/>
  <rdfs:range rdf:resource="#Meter"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class that defines which meter is the previous of this meter</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasTempo">
  <rdfs:subPropertyOf rdf:resource="#hasMusicalFeature"/>
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="#Tempo"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class the defined the tempo of a track</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasRhythmChange">
  <rdfs:subPropertyOf rdf:resource="#hasMusicalFeature"/>
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="#RhythmChange"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class that represents the rhythm change of a Track/meter</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasNotes">
  <rdfs:subPropertyOf rdf:resource="#hasMusicalFeature"/>
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="#Notes"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class representing the notes of a track/meter</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasMelodyRange">
  <rdfs:subPropertyOf rdf:resource="#hasMusicalFeature"/>
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="#MelodyRange"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class tha represents the melody range of a Track/meter</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasMelodyDirection">
  <rdfs:subPropertyOf rdf:resource="#hasMusicalFeature"/>
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="#MelodyDirection"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class tha represents the melody direction of a Track/meter</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasInterval">
  <rdfs:subPropertyOf rdf:resource="#hasMusicalFeature"/>
  <rdfs:domain rdf:resource="#IntervalCount"/>
  <rdfs:range rdf:resource="#Intervals"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class to link the interval count to the specific interval</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasMelodyCadence">
  <rdfs:subPropertyOf rdf:resource="#hasMusicalFeature"/>
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="#MelodyCadence"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class tha represents the melody cadence of a Track/meter</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasArticulation">
  <rdfs:subPropertyOf rdf:resource="#hasMusicalFeature"/>
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="#Articulation"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class tha represents the articulation of a Track/meter</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasDynamicShape">
  <rdfs:subPropertyOf rdf:resource="#hasMusicalFeature"/>
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="#DynamicShape"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class representing the dynamic shape of a track/meter</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasDynamics">
  <rdfs:subPropertyOf rdf:resource="#hasMusicalFeature"/>
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="#Dynamics"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class representing the dynamics of a track/meter</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasDirectInstructions">
  <rdfs:subPropertyOf rdf:resource="#hasMusicalFeature"/>
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="#DirectInstructions"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class representing the direct instruction of a track</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasAveragePitch">
  <rdfs:subPropertyOf rdf:resource="#hasMusicalFeature"/>
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="#Pitch"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class tha represents the overall pitch of a Track/meter</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasPitchRange">
  <rdfs:subPropertyOf rdf:resource="#hasMusicalFeature"/>
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="#PitchRange"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class tha represents the overall pitch Range of a Track/meter</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasTimbre">
  <rdfs:subPropertyOf rdf:resource="#hasMusicalFeature"/>
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="#Timbre"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class tha represents the overall timbre of a Track/meter</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasMode">
  <rdfs:subPropertyOf rdf:resource="#hasMusicalFeature"/>
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="#Mode"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class that defines the mode of a track</rdfs:comment>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#trackHasAverageValence">
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class that defines the overall valence of the track</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#trackHasAverageArousal">
  <rdfs:domain rdf:resource="#Track"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class that defines the overall arousal of the track</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#meterHasPosition">
  <rdfs:domain rdf:resource="#Meter"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasValenceValue">
  <rdfs:domain rdf:resource="#Meter"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class that defines a decimal number representing the valence value of the meter</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasArousalValue">
  <rdfs:domain rdf:resource="#Meter"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class that defines a decimal number representing the arousal value of the meter</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasBPM">
  <rdfs:domain rdf:resource="#Tempo"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasIntervalCount">
  <rdfs:domain rdf:resource="#IntervalCount"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class to count intervals</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasIntervalsRatio">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Intervals"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Data Property for the ratio of intervals</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasDuration">
  <rdfs:domain rdf:resource="#Intervals"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An integer represeting the duration of the interval - the note with the biggest duration</rdfs:comment>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Track">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class representing a musical track</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Meter">
  <rdfs:subClassOf rdf:resource="#Track"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class representing a meter in a track</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#MusicalFeature">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class representing a musical feature</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#EmotionalEffect">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class that represents an emotional effect</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Arousal">
  <rdfs:subClassOf rdf:resource="#EmotionalEffect"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class representing arousal effect</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Arousal</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#Valence">
  <rdfs:subClassOf rdf:resource="#EmotionalEffect"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class that represents valence effect</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#LowArousal">
  <rdfs:subClassOf rdf:resource="#Arousal"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Subclass representing Low Arousal</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#VeryLowArousal">
  <rdfs:subClassOf rdf:resource="#Arousal"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Subclass representing Very Low Arousal</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#MediumArousal">
  <rdfs:subClassOf rdf:resource="#Arousal"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Subclass representing Medium Arousal</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#HighArousal">
  <rdfs:subClassOf rdf:resource="#Arousal"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Subclass representing High Arousal</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#VeryHighArousal">
  <rdfs:subClassOf rdf:resource="#Arousal"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Subclass representing Very High Arousal</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#PositiveValence">
  <rdfs:subClassOf rdf:resource="#Valence"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class representing emotions with positive Valence, e.g happiness</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#MediumPositiveValence">
  <rdfs:subClassOf rdf:resource="#PositiveValence"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Subclass representing emotions with Medium positive Valence</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#VeryPositiveValence">
  <rdfs:subClassOf rdf:resource="#PositiveValence"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Subclass representing Emotions with very positive valence</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#NegativeValence">
  <rdfs:subClassOf rdf:resource="#Valence"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class representing emotions with negative Valence</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#LowNegativeValence">
  <rdfs:subClassOf rdf:resource="#NegativeValence"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class representing emotions with Low negative valence ()</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#VeryNegativeValence">
  <rdfs:subClassOf rdf:resource="#NegativeValence"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class representing emotions with Very negative valence ()</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#VeryPositiveValenceMeter">
  <rdfs:subClassOf rdf:resource="#Meter"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Meter"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#hasValenceValue"/>
          <owl:someValuesFrom>
            <rdfs:Datatype>
              <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
              <owl:withRestrictions>
                <rdf:Description>
                  <rdf:first>
                    <rdf:Description>
                      <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">3.9</xsd:minInclusive>
                    </rdf:Description>
                  </rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:Description>
              </owl:withRestrictions>
            </rdfs:Datatype>
          </owl:someValuesFrom>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class representing meters with very positive valence</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#LowPositiveValenceMeter">
  <rdfs:subClassOf rdf:resource="#Meter"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Meter"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#hasValenceValue"/>
          <owl:someValuesFrom>
            <rdfs:Datatype>
              <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
              <owl:withRestrictions>
                <rdf:Description>
                  <rdf:first>
                    <rdf:Description>
                      <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.25</xsd:minInclusive>
                    </rdf:Description>
                  </rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:Description>
              </owl:withRestrictions>
            </rdfs:Datatype>
          </owl:someValuesFrom>
        </owl:Restriction>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#hasValenceValue"/>
          <owl:someValuesFrom>
            <rdfs:Datatype>
              <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
              <owl:withRestrictions>
                <rdf:Description>
                  <rdf:first>
                    <rdf:Description>
                      <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">3.9</xsd:maxExclusive>
                    </rdf:Description>
                  </rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:Description>
              </owl:withRestrictions>
            </rdfs:Datatype>
          </owl:someValuesFrom>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class representing meters with low positive valence</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#LowNegativeValenceMeter">
  <rdfs:subClassOf rdf:resource="#Meter"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">

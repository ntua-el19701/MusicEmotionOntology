              <owl:withRestrictions>
                <rdf:Description>
                  <rdf:first>
                    <rdf:Description>
                      <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.24</xsd:maxExclusive>
                    </rdf:Description>
                  </rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:Description>
              </owl:withRestrictions>
            </rdfs:Datatype>
          </owl:someValuesFrom>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class representing meters with low positive valence</rdfs:comment>
